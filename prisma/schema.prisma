generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model startup {
  uuid      String @id @db.Uuid
  name      String @db.VarChar
  website   String @db.VarChar
  type      String @db.VarChar
  positions Json[] @db.Json
  img       String @db.VarChar
}
